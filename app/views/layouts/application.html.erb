<!DOCTYPE html>
<html>
	<head>
  		<title>Wanted List</title>
  		<%= stylesheet_link_tag    "application", :media => "all" %>
  		<%= javascript_include_tag "application" %>
  		<%= csrf_meta_tags %>
  		<%= render 'layouts/shim' %>
	</head>
	<body>
		<%= render 'layouts/navbar' %>
		
		<div class="container">
			<% flash.each do |key, value| %>
        		<div><%= value %></div>
      		<% end %>
			<%= yield %>
			<%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>
